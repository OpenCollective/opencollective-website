{
  "name": "opencollective-website",
  "version": "0.1.331",
  "engines": {
    "node": "6.11.0",
    "npm": "3.10.10"
  },
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ]
  },
  "scripts": {
    "start": "node server/dist",
    "start:api": "sh -c \"cd $API_DIR && npm start > api.log &\"",
    "dev": "npm run build && WEBSITE_PORT=3000 nf start -j Procfile_dev",
    "hint": "eslint '**/*.js' --quiet",
    "pretest": "npm run hint && gulp build && node nightwatch.conf.js && npm run start:api",
    "test": "npm run test:server && npm run test:unit",
    "test:server": "mocha --opts test/unit/mocha.opts test/server",
    "test:unit": "mocha test/unit/ --opts test/unit/mocha.opts --reporter-options mochaFile=test/output/junit/junit.xml",
    "posttest": "npm run test:e2e",
    "test:e2e": "sh -c \"npm start > server.log\" & PID=$! && echo \"PID is $PID\" && sleep 5 && npm run nightwatch && kill $PID",
    "nightwatch": "./node_modules/nightwatch/bin/nightwatch --config nightwatch.conf.js",
    "build": "gulp build && npm run build:server && npm run build:frontend",
    "build:server": "rm -rf server/dist && mkdir server/dist && babel -D ./server/src -d ./server/dist && touch server/src/index.js",
    "build:frontend": "mkdir -p frontend/dist/js && browserify frontend/src/client.js | uglifyjs -cm --screw-ie8 > frontend/dist/js/bundle.js ",
    "watch:js": "mkdir -p frontend/dist/js && watchify frontend/src/client.js -o frontend/dist/js/bundle.js -dv",
    "watch:server": "nodemon -e js,hbs -w server/src -w frontend/src",
    "watch:test": "mocha test/unit/ --opts test/unit/mocha.opts --watch",
    "postinstall": "npm run build",
    "git:clean": "./scripts/git_clean.sh",
    "deploy:staging": "bash -x scripts/deploy.sh staging",
    "deploy:production": "bash -x scripts/deploy.sh production"
  },
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "babelify": "8.0.0",
    "basscss": "8.0.4",
    "basscss-background-images": "0.1.1",
    "bluebird": "3.5.1",
    "browserify": "16.2.2",
    "cache-bust": "0.0.2",
    "cached-request": "1.1.2",
    "chai": "4.1.2",
    "classnames": "2.2.5",
    "compression": "1.7.2",
    "config": "1.30.0",
    "cors": "2.8.4",
    "cssnano": "3.10.0",
    "ejs": "2.6.1",
    "envify": "4.1.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-mocha": "5.0.0",
    "eslint-plugin-react": "7.8.2",
    "express": "4.16.3",
    "express-hbs": "1.0.4",
    "express-server-status": "1.0.3",
    "express-session": "1.15.6",
    "gm": "1.23.1",
