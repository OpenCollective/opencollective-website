    "gulp-watch": "5.0.0",
    "history": "4.7.2",
    "html-pdf": "2.2.0",
    "image-size": "0.6.2",
    "image-to-ascii": "3.0.11",
    "immutable": "3.8.2",
    "isomorphic-fetch": "2.2.1",
    "joi": "13.3.0",
    "jwt-decode": "2.2.0",
    "jwt-simple": "0.5.1",
